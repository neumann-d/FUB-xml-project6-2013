{
  "manifest_version": 2,

  "name": "SemanticBrowser",
  "description": "Surfing the Web of Data",
  "version": "0.1",

  "permissions": [
    "webNavigation",
    "http://127.0.0.1/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
          
   "background": {
    "scripts": ["js/libs/jquery-1.9.0/jquery.min.js","eventPage.js"],
    "persistent": false
  }
}